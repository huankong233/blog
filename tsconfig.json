{
  "extends": "tsconfig-vuepress/base.json",
  "compilerOptions": {
    "target": "ES2022",
    "lib": ["DOM", "ES2022"],
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "types": ["vuepress/client-types"],
    "noImplicitAny": false,
    "noEmitOnError": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true
  },
  "include": ["**/.vuepress/**/*"],
  "exclude": ["node_modules", ".cache", ".temp", "dist"]
}
